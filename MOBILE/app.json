{
  "expo": {
    "name": "Mochila Inteligente",
    "slug": "mochila-inteligente",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/mochila-PI-sem-fundo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Este aplicativo precisa de acesso à câmera para escanear QR Codes."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "edgeToEdgeEnabled": true,
      "package": "com.luispedrocarrocini.mochilainteligente",
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5aeac9fb-5fff-4923-9a5f-bdc5419920fa"
      }
    },
    "plugins": [
      "expo-font"
    ]
  }
}